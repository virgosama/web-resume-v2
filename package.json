{
  "name": "web-resume-v2",
  "version": "1.0.0",
  "description": "",
  "main": "script.js",
  "scripts": {
    "watch:sass": "node-sass --recursive sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autprefixer -b 'last 10 versions' css/style.comp.css css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "update:sprite": "svg-sprite -s --symbol-dest img --symbol-sprite sprite.svg svg/*.svg",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "concat": "^1.0.3",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12"
  }
}
